{
  "name": "tina-schema-generator",
  "version": "1.0.0",
  "description": "Generate TinaCMS schemas for AstroJS projects.",
  "type": "commonjs",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "bin": {
    "tina-schema-generator": "dist/index.cjs"
  },
  "scripts": {
    "build": "rollup -c",
    "generate-tina-schema-cjs": "node dist/scripts/generate-tina-schema.cjs",
    "generate-tina-schema-mjs": "node dist/scripts/generate-tina-schema.mjs"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.2",
    "axios": "^1.7.2",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^3.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "astro",
    "tina",
    "cms",
    "schema",
    "generator"
  ],
  "author": "Al Murad Uzzaman",
  "license": "ISC"
}
